{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest-pubnub/src",
  "monorepo": true,
  "root": "apps/nest-pubnub",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/nest-pubnub/tsconfig.app.json"
  },
  "projects": {
    "nest-pubnub": {
      "type": "application",
      "root": "apps/nest-pubnub",
      "entryFile": "main",
      "sourceRoot": "apps/nest-pubnub/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-pubnub/tsconfig.app.json"
      }
    },
    "my-app": {
      "type": "application",
      "root": "apps/my-app",
      "entryFile": "main",
      "sourceRoot": "apps/my-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/my-app/tsconfig.app.json"
      }
    },
    "pub-nub": {
      "type": "library",
      "root": "libs/pub-nub",
      "entryFile": "index",
      "sourceRoot": "libs/pub-nub/src",
      "compilerOptions": {
        "tsConfigPath": "libs/pub-nub/tsconfig.lib.json"
      }
    }
  }
}